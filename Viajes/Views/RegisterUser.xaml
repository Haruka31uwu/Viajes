<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Viajes.Views.RegisterUser"
            Visual="Material">
    <StackLayout Orientation="Vertical" Margin="20" VerticalOptions="Center" >
        <Entry Placeholder="Email" x:Name="txtemail"/>
        <Entry Placeholder="Password" x:Name="txtpass" />
        <Button Text="Registrar" Clicked="Button_Clicked" />
    </StackLayout>
    
</ContentPage>