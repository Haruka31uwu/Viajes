<?xml version="1.0" encoding="utf-8" ?>
<FlyoutPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Viajes.Views.Main.MainPage"
             xmlns:pages="clr-namespace:Viajes.Views.Main">
  <FlyoutPage.Flyout>
    <pages:MainPageFlyout x:Name="FlyoutPage" />
  </FlyoutPage.Flyout>
  <FlyoutPage.Detail>
    <NavigationPage>
      <x:Arguments>
        <pages:MainPageDetail/>
      </x:Arguments>
    </NavigationPage>
  </FlyoutPage.Detail>  
</FlyoutPage>