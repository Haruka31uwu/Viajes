<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Viajes.Views.Main.MainPages.ViewPage">
    <ContentPage.Content>
        <StackLayout Margin="20">
            <Image x:Name="img" HeightRequest="300"/>
            <Label x:Name="sn" HorizontalOptions="Center"/>
            <Label x:Name="des" HorizontalOptions="Center"/>
            <Label x:Name="dt" HorizontalOptions="Center">
                <Label.GestureRecognizers>
                    <TapGestureRecognizer Tapped="ClickGestureRecognizer_Clicked"/>
                </Label.GestureRecognizers>
            </Label>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>